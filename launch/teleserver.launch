<launch>
  <node name="image_view" pkg="image_view" type="image_view" respawn="false" output="screen">
    <remap from="image" to="/camera/image"/>
    <param name="autosize" value="false" />
    <param name="image_transport" value="compressed" />
  </node>

  <arg name="ns" default="audio_mp3"/>
  <group ns="$(arg ns)">
  <node name="audio_play" pkg="audio_play" type="audio_play" output="screen">
    <param name="dst" value="alsasink"/>
  </node>
  </group>

  <node name="teleserver_key" pkg="telemouse" type="teleserver_key.py"/>
  <node name="audio_disconf" pkg="telemouse" type="audio_disconf.py"/>

</launch>
